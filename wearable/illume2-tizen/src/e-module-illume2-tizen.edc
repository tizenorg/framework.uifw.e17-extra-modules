   group
     {
        images.image: "multi_window_minimize_normal.png" COMP;
        images.image: "multi_window_minimize_press.png" COMP;
        name: "mw_minimize_mode";

        parts
          {
             part
               {
                  name:  "bg";
                  mouse_events: 1;
                  scale: 1;
                  type: IMAGE;
                  description
                    {
                       state: "default" 0.0;
                       min: 112 112;
                       max: 112 112;
                       fixed: 1 1;
                       rel1 { relative: 0.0 0.0; }
                       rel2 { relative: 1.0 1.0; }
                       image.normal: "multi_window_minimize_normal.png";
                       //image.middle: SOLID;
                    }
                  description
                    {
                       state: "focused" 0.0;
                       inherit: "default" 0.0;
                       image.normal: "multi_window_minimize_press.png";
                    }
               }
             part
               {
                  type: SWALLOW;
                  name:  "swallow_icon";
                  scale: 1;
                  description
                    {
                       state: "default" 0.0;
                       min: 102 102;
                       max: 102 102;
                       aspect: 1.0 1.0;
                       fixed: 1 1;
                       align: 0.5 0.5;
                       rel1 { relative: 0.0 0.0; to: "bg"; offset: 4 4; }
                       rel2 { relative: 1.0 1.0; to: "bg"; offset: -4 -4; }
                    }
               }
         }
        programs
           {
              program
                {
                   source: "illume2";
                   name:   "icon_focused";
                   signal: "pin.focused";
                   action: STATE_SET "focused" 0.0;
                   target: "bg";
                }
              program
                {
                   source: "illume2";
                   name:   "icon_losefocused";
                   signal: "pin.losefocus";
                   action: STATE_SET "default" 0.0;
                   target: "bg";
                }
              program
                {
                   source: "illume2";
                   name:   "icon_play_sound";
                   signal: "pin.sound";
                   action: PLAY_SAMPLE "touch_sound" 1.0;
                }
           }
     }

   group
     {
        images.image: "mw_frame_press.png" COMP;
        images.image: "mw_frame_press_overscroll.png" COMP;
        images.image: "mw_frame_handler_press.png" COMP;
        name: "new_shadow";
        max: 720 720;
        parts
          {
             part
               {
                  name: "opacity_rect";
                  type: RECT;
                  description
                    {
                       state: "default" 0.0;
                       color: 255 255 255 255;
                    }
               }
             part
               {
                  name: "resize_border";
                  type: IMAGE;
                  clip_to: "opacity_rect";
                  description
                    {
                       state: "default" 0.0;
                       image.normal: "mw_frame_press.png";
                       image.border: 3 3 3 3;
                       image.middle: NONE;
                    }
                  description
                    {
                       state: "avail" 0.0;
                       inherit: "default" 0.0;
                    }
                  description
                    {
                       state: "notavail" 0.0;
                       image.normal: "mw_frame_press_overscroll.png";
                       image.border: 3 3 3 3;
                       image.border_scale: 0;
                    }
               }
             part
               {
                  name: "resize_handle";
                  type: IMAGE;
                  clip_to: "opacity_rect";
                  description
                    {
                       state: "default" 0.0;
                       min: 36 36;
                       max:  36 36;
                       image.normal: "mw_frame_handler_press.png";
                       align: 1 1;
                    }
                  description
                    {
                       state: "default" 0.0;
                       min: 36 36;
                       max:  36 36;
                       image.normal: "mw_frame_handler_press.png";
                       align: 1 1;
                       visible: 0;
                    }
                  description
                    {
                       state: "visible" 0.0;
                       inherit: "default" 0.0;
                       visible: 1;
                    }
                  description
                    {
                       state: "degree_90" 0.0;
                       inherit: "visible" 0.0;
                       map.on: 1;
                       map.rotation.z: 270;
                       align: 1 0;
                    }
                  description
                    {
                       state: "degree_180" 0.0;
                       inherit: "visible" 0.0;
                       map.on: 1;
                       map.rotation.z: 180;
                       align: 0 0;
                    }
                  description
                    {
                       state: "degree_270" 0.0;
                       inherit: "visible" 0.0;
                       map.on: 1;
                       map.rotation.z: 90;
                       align: 0 1;
                    }
               }
          }

          programs
           {
               program
                 {
                    source: "illume2";
                    action: STATE_SET "visible" 0;
                    signal: "resize,angle";
                    target: "resize_handle";
                 }
               program
                 {
                    source: "illume2";
                    action: STATE_SET "degree_90" 0;
                    signal: "resize,angle,90";
                    target: "resize_handle";
                 }
               program
                 {
                    source: "illume2";
                    action: STATE_SET "degree_180" 0;
                    signal: "resize,angle,180";
                    target: "resize_handle";
                 }
               program
                 {
                    source: "illume2";
                    action: STATE_SET "degree_270" 0;
                    signal: "resize,angle,270";
                    target: "resize_handle";
                 }
               program
                 {
                    source: "illume2";
                    action: STATE_SET "avail" 0;
                    signal: "resize,avail";
                    target: "resize_border";
                 }
               program
                 {
                    source: "illume2";
                    action: STATE_SET "notavail" 0;
                    signal: "resize,notavail";
                    target: "resize_border";
                 }
           }
     }

   /* for the tiling */
   group { name: "ly_bar_resize";
      images {
         image: "ly_bar_resize.png" COMP;
      }
      max: 720 7;
      parts {
         part { name: "clipper";
            type: RECT;
            description { state: "default" 0.0;
               visible: 1;
               color: 255 255 255 1;
            }
            description { state: "visible" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
         }
         part { name: "shower";
            type: IMAGE;
            clip_to: "clipper";
            description { state: "default" 0.0;
               image.normal: "ly_bar_resize.png";
            }
         }
      }
      programs {
         program { name: "show";
            signal: "visible,on";
            source: "illume2";
            action: STATE_SET "visible" 0.0;
            transition: DECELERATE 0.4;
            filter: "clipper" "default";
            target: "clipper";
         }
         program { name: "hide";
            signal: "visible,off";
            source: "illume2";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.8;
            filter: "clipper" "visible";
            target: "clipper";
         }
      }
   }

