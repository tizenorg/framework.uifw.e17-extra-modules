   group { name: "ly_active_stress";
      images {
         image: "ly_active_line.png" COMP;
         image: "ly_active_rect.png" COMP;
      }
      parts {
         part { name: "clipper";
            type: RECT;
            description { state: "default" 0.0;
               visible: 1;
               color: 255 255 255 1;
               rel1 { relative: 0 0; }
               rel2 { relative: 1 1; }
            }
            description { state: "visible" 0.0;
               visible: 1;
               color: 255 255 255 255;
               rel1 { relative: 0 0; }
               rel2 { relative: 1 1; }
            }
         }
         part { name: "clipper_rect";
            type: RECT;
            description { state: "default" 0.0;
               visible: 1;
               color: 255 255 255 1;
               rel1 { relative: 0 0; }
               rel2 { relative: 1 1; }
            }
            description { state: "visible" 0.0;
               visible: 1;
               color: 255 255 255 255;
               rel1 { relative: 0 0; }
               rel2 { relative: 1 1; }
            }
         }
         part { name: "line";
            type: IMAGE;
            clip_to: "clipper";
            mouse_events: 0;
            description { state: "default" 0.0;
               image.normal: "ly_active_line.png";
               image.border: 7 7 7 7;
               image.middle: NONE;
            }
         }
         part { name: "rect";
            type: IMAGE;
            clip_to: "clipper_rect";
            mouse_events: 0;
            description { state: "default" 0.0;
               image.normal: "ly_active_rect.png";
               rel1 { relative: 0 0; offset: 7 7;   to: "line"; }
               rel2 { relative: 1 1; offset: -7 -7; to: "line"; }
            }
         }
      }
      programs {
         program { name: "show";
            signal: "visible,on,line";
            source: "illume2";
            action: STATE_SET "visible" 0.0;
            transition: DECELERATE 0.2;
            target: "clipper";
         }
         program { name: "hide";
            signal: "visible,off,line";
            source: "illume2";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.2;
            target: "clipper";
         }
         program { name: "show.rect";
            signal: "visible,on,rect";
            source: "illume2";
            action: STATE_SET "visible" 0.0;
            transition: ACCELERATE 0.2;
            target: "clipper_rect";
         }
         program { name: "hide.rect";
            signal: "visible,off,rect";
            source: "illume2";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.2;
            target: "clipper_rect";
