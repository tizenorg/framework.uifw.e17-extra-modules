   group { name: "ly_bar_resize";
      images {
         image: "ly_bar_resize.#.png" COMP;
      }
      max: 720 7;
      parts {
         part { name: "clipper";
            type: RECT;
            description { state: "default" 0.0;
               visible: 1;
               color: 255 255 255 1;
            }
            description { state: "visible" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
         }
         part { name: "shower";
            type: IMAGE;
            clip_to: "clipper";
            description { state: "default" 0.0;
               image.normal: "ly_bar_resize.#.png";
            }
         }
      }
   }

   group { name: "ly_bt_resize";
      images {
         image: "ly_bt_resize.png" COMP;
      }
      max: 100 100;
      parts {
         part { name: "clipper";
            type: RECT;
            description { state: "default" 0.0;
               visible: 1;
               color: 255 255 255 1;
            }
            description { state: "visible" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
         }
         part { name: "shower";
            type: IMAGE;
            clip_to: "clipper";
            description { state: "default" 0.0;
            }
         }
         part { name: "img";
            type: IMAGE;
            clip_to: "clipper";
            description { state: "default" 0.0;
               image.normal: "ly_bt_resize.png";
               rel1 { relative: 0.5 0.5; to: "shower"; offset: -35 -35;}
               rel2 { relative: 0.5 0.5; to: "shower"; offset:  35  35;}
            }
         }
      }
      programs {
         program { name: "show";
            signal: "visible,on";
            source: "illume2";
            action: STATE_SET "visible" 0.0;
            transition: DECELERATE 0.2;
            filter: "clipper" "default";
            target: "clipper";
         }
         program { name: "hide";
            signal: "visible,off";
            source: "illume2";
            action: STATE_SET "default" 0.0;
            filter: "clipper" "visible";
            target: "clipper";
         }
         program {
            signal: "reached,max_min,on";
            source: "illume2";
            action: STATE_SET "max_min" 0.0;
            transition: ACCELERATE 0.2;
            filter: "clipper" "visible";
            target: "clipper";
         }
         program {
            signal: "reached,max_min,off";
            source: "illume2";
            action: STATE_SET "visible" 0.0;
            transition: ACCELERATE 0.2;
            filter: "clipper" "max_min";
            target: "clipper";
         }
      }
   }
